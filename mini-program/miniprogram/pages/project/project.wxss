.container {
  padding: 0rpx 30rpx 30rpx;
  min-height: 100vh;
  background: #f9f9f9;
  box-sizing: border-box;
  color: #333;
}
.title {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 80rpx;
  background: #4187f2;
  border-radius: 8px 8px 0px 0px;
  font-size: 34rpx;
  color: #fff;
}
.item {
  margin-top: 16rpx;
}
.block {
  width: 100%;
  padding: 0 16rpx 16rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 0px 0px 8px 8px;
}
.image {
  display: block;
  width: 100%;
  margin-top: 12rpx;
}
.h5 {
  font-size: 30rpx;
  padding: 10rpx 0;
}
.h6 {
  font-size: 26rpx;
  padding: 10rpx 0;
}
.text {
  font-size: 30rpx;
  padding: 10rpx 0;
  line-height: 50rpx;
}
